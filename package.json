{
	"name": "ez-ui-elements",
	"version": "0.1.0",
	"description": "Create easy User Interface Elements that are repetitive or complex to generate",
	"private": false,
	"main": "index.js",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	},
	"dependencies": {
		"react": "^16.4.0",
		"react-dom": "^16.8.6",
		"react-scripts": "3.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"compile": "babel src --out-file index.js",
		"dist": "rm -rf dist && mkdir dist &&  babel      ./src/ -d dist --copy-files",
		"publish:npm": "rm -rf dist && mkdir dist &&  babel      ./src/ -d dist --copy-files"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"babel-preset-env": "^1.7.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/egomezm/ez-ui-elements.git"
	},
	"keywords": [
		"chart"
	],
	"author": "Eduardo Gomez - EGM Solutions",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/egomezm/ez-ui-elements/issues"
	},
	"homepage": "https://github.com/egomezm/ez-ui-elements#readme"
}
